		["Auto Voter (CHEAT MODE)"] = {
			["iconSource"] = 0,
			["wagoID"] = "dW5U0N3HP",
			["parent"] = "Aeon Remnants - Among Us Helper",
			["preferToUpdate"] = false,
			["customText"] = "",
			["yOffset"] = -70,
			["anchorPoint"] = "CENTER",
			["cooldownSwipe"] = true,
